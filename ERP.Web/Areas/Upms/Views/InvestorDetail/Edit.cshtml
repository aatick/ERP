    function InvestorIntroducerSave() {

        var InvestorId = $("#IntroInvestorId").val();
        var IntroducerId = $("#hdnIntroducerId").val(); // Investor or employee Id
        var IntroType = $("#ddlIntroType").val();
        var Id = $("#IntroducerId").val();// If introducer is alredy save then Id comes from InvestorIntroducer table or  0

        $.ajax({
            type: 'GET',
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("InvestorIntroducerEdit", "InvestorDetail", "http")',
            data: { Id: Id, InvestorId: InvestorId, IntroducerId: IntroducerId, IntroType: IntroType },
            dataType: 'json',
            async: true,
            success: function (data) {
                if (data.Result == "Ok") {
                    $("#hdnIntroducerId").val("");
                    $("#hdnInvestorId").val(data.InvestorId);
                    $(".clInvestorId").val(data.InvestorId);
                    $("#IntroducerName").val("");
                    $("#IntroducerContactNo").val("");
                    $("#IntroducerInvestorCode").val("");
                    $("#Employee").val("");
                    //   
                    //$.alert.open("info", "Save successfull.  ...");
                    $("#SuccIntroMsg").show(800).fadeToggle(3000);
                }
                else {
                    $.alert.open("error",data.Result);
                    //$(".failed").show(800).fadeToggle(3000);
                }
            },
            error: function (request, status, error) {
                alert(request.statusText + "/" + request.statusText + "/" + error);
            }
        });
    }



    $(document).ready(function () {

        //alert($("#IntroducerInvestorId").val());
        if ($("#IntroducerType").val() == "Emp") {
            $("#ddlIntroType").val("Emp");
            $("#divInvestorIntro").hide()
            $("#divEmpIndro").show();
            $("#hdnIntroducerId").val($("#IntroEmployeeId").val());
        }
        else
        {
            $("#ddlIntroType").val("Inv");
            $("#divEmpIndro").hide();
            $("#divInvestorIntro").show()
           
            $("#hdnIntroducerId").val($("#IntroducerInvestorId").val());
        }

        $("#btnIntroducerSave").click(function (e) {
            e.preventDefault();
            if ($("#IntroInvestorId").val() != null && $("#IntroInvestorId").val() != "") {
                if ($("#hdnIntroducerId").val() != "" && $("#hdnIntroducerId").val() != null) {
                    InvestorIntroducerSave();
                }
                else {
                    $.alert.open("error", "Insert required field");
                }
            }
            else {
                $.alert.open("error", "Firstly save investor info");
            }

        });


       $("#Employee").blur(function () {

            var EmployeeCode = $("#Employee").val();
            if (EmployeeCode != "")
            {
                $.ajax({
                    type: 'GET',
                    contentType: "application/json; charset=utf-8",
                    url: '@Url.Action("GetIntroducerEmpInfo", "InvestorDetail", "http")',
                    data: { EmployeeCode: EmployeeCode },
                    dataType: 'json',
                    async: true,
                    success: function (Emp) {
                        if (Emp.length == 1) {
                            $.each(Emp, function (id, option) {
                                $("#hdnIntroducerId").val(option.Id);
                                $("#IntroducerContactNo").val(option.PhoneNo);
                                $("#IntroducerName").val(option.EmployeeName);

                            });
                        }
                        else {
                            $("#hdnIntroducerId").val("");
                            $("#IntroducerContactNo").val("");
                            $("#IntroducerName").val("");
                            $.alert.open("error", "Employee not found");
                        }

                    },
                    error: function (request, status, error) {
                        alert(request.statusText + "/" + request.statusText + "/" + error);
                    }
                });
            }

        });


         $("#IntroducerInvestorCode").blur(function () {
            var IntroducerCode = $("#IntroducerInvestorCode").val();
            if (IntroducerCode != "")
            {
                $.ajax({
                    type: 'GET',
                    contentType: "application/json; charset=utf-8",
                    url: '@Url.Action("GetIntroducerInfo", "InvestorDetail", "http")',
                    data: { IntroducerCode: IntroducerCode },
                    dataType: 'json',
                    async: true,
                    success: function (Inves) {
                        if (Inves.length == 1) {
